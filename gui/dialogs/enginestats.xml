<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>
    <name>enginestats</name>
    <modal type="bool">false</modal>
    <layout>vbox</layout>
	<default-padding>3</default-padding>
	<group>
		<layout>hbox</layout>
		<default-padding>1</default-padding>

		<empty><stretch>true</stretch></empty>

		<text>
			<label>Engine Stats</label>
		</text>

		<empty><stretch>true</stretch></empty>

		<button>
			<legend></legend>
			<key>Esc</key>
			<pref-width>16</pref-width>
			<pref-height>16</pref-height>
			<border>2</border>
			<binding>
				<command>dialog-close</command>
			</binding>
		</button>
	</group>

    <hrule/>

    <group>
        <layout>table</layout>

        <text>
            <row>0</row>
            <col>0</col>
            <label></label>
        </text>
        <text>
            <row>0</row>
            <col>1</col>
            <label>Left Engine</label>
        </text>
        <text>
            <row>0</row>
            <col>2</col>
            <label>Right Engine</label>
        </text>

        <text>
            <row>1</row>
            <col>0</col>
            <label>Engine RPM</label>
            <halign>left</halign>
        </text>
        <text>
            <row>1</row>
            <col>1</col>
            <label>000000</label>
            <property>/fdm/jsbsim/propulsion/engine[0]/engine-rpm</property>
            <format>%6.1f</format>
            <live type="bool">true</live>
            <halign>right</halign>
        </text>
        <text>
            <row>1</row>
            <col>2</col>
            <label>000000</label>
            <property>/fdm/jsbsim/propulsion/engine[1]/engine-rpm</property>
            <format>%6.1f</format>
            <live type="bool">true</live>
            <halign>right</halign>
        </text>

        <text>
            <row>2</row>
            <col>0</col>
            <label>Prop RPM</label>
            <halign>left</halign>
        </text>
        <text>
            <row>2</row>
            <col>1</col>
            <label>000000</label>
            <property>/fdm/jsbsim/propulsion/engine[0]/propeller-rpm</property>
            <format>%6.1f</format>
            <live type="bool">true</live>
            <halign>right</halign>
        </text>
        <text>
            <row>2</row>
            <col>2</col>
            <label>000000</label>
            <property>/fdm/jsbsim/propulsion/engine[1]/propeller-rpm</property>
            <format>%6.1f</format>
            <live type="bool">true</live>
            <halign>right</halign>
        </text>

        <text>
            <row>3</row>
            <col>0</col>
            <label>Prop Pitch</label>
            <halign>left</halign>
        </text>
        <text>
            <row>3</row>
            <col>1</col>
            <label>000000</label>
            <property>/fdm/jsbsim/propulsion/engine[0]/blade-angle</property>
            <format>%6.1f</format>
            <live type="bool">true</live>
            <halign>right</halign>
        </text>
        <text>
            <row>3</row>
            <col>2</col>
            <label>000000</label>
            <property>/fdm/jsbsim/propulsion/engine[1]/blade-angle</property>
            <format>%6.1f</format>
            <live type="bool">true</live>
            <halign>right</halign>
        </text>

        <text>
            <row>4</row>
            <col>0</col>
            <label>Advance Ratio</label>
            <halign>left</halign>
        </text>
        <text>
            <row>4</row>
            <col>1</col>
            <label>000000</label>
            <property>/fdm/jsbsim/propulsion/engine[0]/advance-ratio</property>
            <format>%6.4f</format>
            <live type="bool">true</live>
            <halign>right</halign>
        </text>
        <text>
            <row>4</row>
            <col>2</col>
            <label>000000</label>
            <property>/fdm/jsbsim/propulsion/engine[1]/advance-ratio</property>
            <format>%6.4f</format>
            <live type="bool">true</live>
            <halign>right</halign>
        </text>

        <text>
            <row>5</row>
            <col>0</col>
            <label>Thrust Coefficient</label>
            <halign>left</halign>
        </text>
        <text>
            <row>5</row>
            <col>1</col>
            <label>000000</label>
            <property>/fdm/jsbsim/propulsion/engine[0]/thrust-coefficient</property>
            <format>%6.4f</format>
            <live type="bool">true</live>
            <halign>right</halign>
        </text>
        <text>
            <row>5</row>
            <col>2</col>
            <label>000000</label>
            <property>/fdm/jsbsim/propulsion/engine[1]/thrust-coefficient</property>
            <format>%6.4f</format>
            <live type="bool">true</live>
            <halign>right</halign>
        </text>

        <text>
            <row>6</row>
            <col>0</col>
            <label>Helical Tip Mach Number</label>
            <halign>left</halign>
        </text>
        <text>
            <row>6</row>
            <col>1</col>
            <label>000000</label>
            <property>/fdm/jsbsim/propulsion/engine[0]/helical-tip-Mach</property>
            <format>%6.4f</format>
            <live type="bool">true</live>
            <halign>right</halign>
        </text>
        <text>
            <row>6</row>
            <col>2</col>
            <label>000000</label>
            <property>/fdm/jsbsim/propulsion/engine[1]/helical-tip-Mach</property>
            <format>%6.4f</format>
            <live type="bool">true</live>
            <halign>right</halign>
        </text>

        <text>
            <row>7</row>
            <col>0</col>
            <label>Power (BHP)</label>
            <halign>left</halign>
        </text>
        <text>
            <row>7</row>
            <col>1</col>
            <label>000000</label>
            <property>/fdm/jsbsim/propulsion/engine[0]/power-hp</property>
            <format>%6.1f</format>
            <live type="bool">true</live>
            <halign>right</halign>
        </text>
        <text>
            <row>7</row>
            <col>2</col>
            <label>000000</label>
            <property>/fdm/jsbsim/propulsion/engine[1]/power-hp</property>
            <format>%6.1f</format>
            <live type="bool">true</live>
            <halign>right</halign>
        </text>

        <text>
            <row>8</row>
            <col>0</col>
            <label>Thrust (lbf)</label>
            <halign>left</halign>
        </text>
        <text>
            <row>8</row>
            <col>1</col>
            <label>000000</label>
            <property>/fdm/jsbsim/propulsion/engine[0]/thrust-lbs</property>
            <format>%6.1f</format>
            <live type="bool">true</live>
            <halign>right</halign>
        </text>
        <text>
            <row>8</row>
            <col>2</col>
            <label>000000</label>
            <property>/fdm/jsbsim/propulsion/engine[1]/thrust-lbs</property>
            <format>%6.1f</format>
            <live type="bool">true</live>
            <halign>right</halign>
        </text>

    </group>
</PropertyList>
