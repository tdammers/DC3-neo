    </center>
    <axis>
      <x> 0 </x>
      <y> 0 </y>
      <z> 1 </z>
    </axis>
  </animation>

  <animation>
    <type>spin</type>
    <object-name>roueAG</object-name>
    <property>gear/gear[0]/rollspeed-ms</property>
    <factor> 30 </factor>
    <center>
      <x-m> -9.545 </x-m>
      <y-m> -2.668 </y-m>
      <z-m> -4.302 </z-m>
    </center>
    <axis>
      <x>  0 </x>
