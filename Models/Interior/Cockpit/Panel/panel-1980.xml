    <path>Aircraft/Douglas-Dc3/Models/Instruments/gps155.xml</path>
    <offsets>
      <x-m>-0.01</x-m>
      <y-m>0.0</y-m>
      <z-m>0.4</z-m>
      <pitch-deg>0</pitch-deg>
    </offsets>
  </model>
  
  <model>
    <name>gps-switch</name>
    <path>Aircraft/Douglas-Dc3/Models/Interior/Cockpit/Panel/gps-switch.xml</path>
    <offsets>
      <x-m>0.0</x-m>
      <y-m>-0.050</y-m>
      <z-m>0.340</z-m>
      <pitch-deg>0</pitch-deg>
    </offsets>
  </model>
  
  <animation>
    <type>select</type>
    <object-name>gps155</object-name>
    <object-name>gps-switch</object-name>
    <object-name>gps-placard</object-name>
    <condition>
      <or>
        <equals>
          <property>sim/model/config/panel</property>
          <value>gps155</value>
        </equals>
      </or>
    </condition>
  </animation>


