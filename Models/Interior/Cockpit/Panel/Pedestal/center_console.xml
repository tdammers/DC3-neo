  <!-- **********  THROTTLE  **********-->
  <animation>
    <type>rotate</type>
    <object-name>Throt_G</object-name>
    <object-name>throt_g_up</object-name>
    <object-name>throt_g_dwn</object-name>
    <property>controls/engines/engine[0]/throttle</property>
    <factor> 70 </factor>
    <center>
      <x-m> 0.334 </x-m>
      <y-m> 0.000 </y-m>
      <z-m> 0.110 </z-m>
    </center>
    <axis>
      <x>  0 </x>
      <y> -1 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>Throt_D</object-name>
    <object-name>throt_d_up</object-name>
    <object-name>throt_d_dwn</object-name>
    <property>controls/engines/engine[1]/throttle</property>
    <factor> 70 </factor>
    <center>
      <x-m> 0.334 </x-m>
      <y-m> 0.000 </y-m>
      <z-m> 0.110 </z-m>
