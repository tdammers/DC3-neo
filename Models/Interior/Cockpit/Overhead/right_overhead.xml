<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>plaque.ac</path>

  <animation>
    <object-name>plaque</object-name>
  </animation>
  
  <!-- Civilian red version -->
  <animation>
    <type>material</type>
    <object-name>plaque</object-name>
    <condition>
      <equals>
        <property>sim/model/config/version</property>
        <value>civilian-red</value>
      </equals>
    </condition>
    <texture>overhead.png</texture>
  </animation>

  <!-- Civilian gray version -->
  <animation>
    <type>material</type>
    <object-name>plaque</object-name>
    <condition>
      <equals>
        <property>sim/model/config/version</property>
        <value>civilian-gray</value>
      </equals>
    </condition>
    <texture>Liveries/colors_gray.png</texture>
  </animation>

  <!-- Military version -->
  <animation>
    <type>material</type>
    <object-name>plaque</object-name>
    <condition>
      <equals>
        <property>sim/model/config/version</property>
        <value>military</value>
      </equals>
    </condition>
    <texture>Liveries/colors_military.png</texture>
  </animation>

