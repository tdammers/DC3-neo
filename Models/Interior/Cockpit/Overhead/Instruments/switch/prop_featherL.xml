<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>push.ac</path>

  <animation>
    <object-name>tour</object-name>
    <object-name>poussoir</object-name>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>poussoir</object-name>
    <action>
      <button>0</button>
      <binding>
        <command>property-toggle</command>
        <property>controls/engines/engine[0]/propeller-feather</property>
      </binding>
      <binding>
        <command>property-toggle</command>
        <property>sim/model/sound/click</property>
      </binding>
    </action>
  </animation>

  <animation>
    <type>translate</type>
    <object-name>poussoir</object-name>
    <property>controls/engines/engine[0]/propeller-feather-pos</property>
    <factor> -0.01 </factor>
    <axis>
      <x>  1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

</PropertyList>
