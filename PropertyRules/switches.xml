<?xml version="1.0"?>
<PropertyList>
    <filter>
        <type>noise-spike</type>
        <input>controls/electric/battery-switch</input>
        <output>controls/electric/battery-switch-pos</output>
        <max-rate-of-change>5</max-rate-of-change>
    </filter>

    <filter>
        <type>noise-spike</type>
        <input>controls/engines/engine[0]/magnetos</input>
        <output>controls/engines/engine[0]/magnetos-pos</output>
        <max-rate-of-change>5</max-rate-of-change>
    </filter>
    <filter>
        <type>noise-spike</type>
        <input>controls/engines/engine[1]/magnetos</input>
        <output>controls/engines/engine[1]/magnetos-pos</output>
        <max-rate-of-change>5</max-rate-of-change>
    </filter>

    <!-- knobs -->
    <filter>
        <type>noise-spike</type>
        <input>controls/lighting/compass-lights</input>
        <output>controls/lighting/compass-lights-pos</output>
        <max-rate-of-change>5</max-rate-of-change>
    </filter>
    <filter>
        <type>noise-spike</type>
        <input>controls/lighting/formation-lights</input>
        <output>controls/lighting/formation-lights-pos</output>
        <max-rate-of-change>5</max-rate-of-change>
    </filter>

    <!-- switches -->
    <filter>
        <type>noise-spike</type>
        <input>controls/lighting/landing-lights[0]</input>
        <output>controls/lighting/landing-lights-pos[0]</output>
        <max-rate-of-change>10</max-rate-of-change>
    </filter>
    <filter>
        <type>noise-spike</type>
        <input>controls/lighting/landing-lights[1]</input>
        <output>controls/lighting/landing-lights-pos[1]</output>
        <max-rate-of-change>10</max-rate-of-change>
    </filter>
    <filter>
        <type>noise-spike</type>
        <input>controls/lighting/cabin-lights</input>
        <output>controls/lighting/cabin-lights-pos</output>
        <max-rate-of-change>10</max-rate-of-change>
    </filter>
    <filter>
        <type>noise-spike</type>
        <input>controls/lighting/passing-lights</input>
        <output>controls/lighting/passing-lights-pos</output>
        <max-rate-of-change>10</max-rate-of-change>
    </filter>
    <filter>
        <type>noise-spike</type>
        <input>controls/lighting/running-lights</input>
        <output>controls/lighting/running-lights-pos</output>
        <max-rate-of-change>10</max-rate-of-change>
    </filter>
    <filter>
        <type>noise-spike</type>
        <input>controls/lighting/tail-lights</input>
        <output>controls/lighting/tail-lights-pos</output>
        <max-rate-of-change>10</max-rate-of-change>
    </filter>
    <filter>
        <type>noise-spike</type>
        <input>controls/lighting/recognition-lights[0]</input>
        <output>controls/lighting/recognition-lights-pos[0]</output>
        <max-rate-of-change>10</max-rate-of-change>
    </filter>
    <filter>
        <type>noise-spike</type>
        <input>controls/lighting/recognition-lights[1]</input>
        <output>controls/lighting/recognition-lights-pos[1]</output>
        <max-rate-of-change>10</max-rate-of-change>
    </filter>
    <filter>
        <type>noise-spike</type>
        <input>controls/lighting/recognition-lights[2]</input>
        <output>controls/lighting/recognition-lights-pos[2]</output>
        <max-rate-of-change>10</max-rate-of-change>
    </filter>

    <filter>
        <type>noise-spike</type>
        <input>controls/anti-ice/window-heat</input>
        <output>controls/anti-ice/window-heat-pos</output>
        <max-rate-of-change>10</max-rate-of-change>
    </filter>
    <filter>
        <type>noise-spike</type>
        <input>controls/anti-ice/pitot-heat[0]</input>
        <output>controls/anti-ice/pitot-heat-pos[0]</output>
        <max-rate-of-change>10</max-rate-of-change>
    </filter>
    <filter>
        <type>noise-spike</type>
        <input>controls/anti-ice/pitot-heat[1]</input>
        <output>controls/anti-ice/pitot-heat-pos[1]</output>
        <max-rate-of-change>10</max-rate-of-change>
    </filter>
    <filter>
        <type>noise-spike</type>
        <input>controls/anti-ice/prop-heat[0]</input>
        <output>controls/anti-ice/prop-heat-pos[0]</output>
        <max-rate-of-change>10</max-rate-of-change>
    </filter>
    <filter>
        <type>noise-spike</type>
        <input>controls/anti-ice/prop-heat[1]</input>
        <output>controls/anti-ice/prop-heat-pos[1]</output>
        <max-rate-of-change>10</max-rate-of-change>
    </filter>
    <filter>
        <type>noise-spike</type>
        <input>controls/anti-ice/engine[0]/carb-heat</input>
        <output>controls/anti-ice/engine[0]/carb-heat-pos</output>
        <max-rate-of-change>10</max-rate-of-change>
    </filter>
    <filter>
        <type>noise-spike</type>
        <input>controls/anti-ice/engine[1]/carb-heat</input>
        <output>controls/anti-ice/engine[1]/carb-heat-pos</output>
        <max-rate-of-change>10</max-rate-of-change>
    </filter>

    <filter>
        <type>noise-spike</type>
        <input>controls/engines/engine[0]/oil-dilution</input>
        <output>controls/engines/engine[0]/oil-dilution-pos</output>
        <max-rate-of-change>10</max-rate-of-change>
    </filter>
    <filter>
        <type>noise-spike</type>
        <input>controls/engines/engine[1]/oil-dilution</input>
        <output>controls/engines/engine[1]/oil-dilution-pos</output>
        <max-rate-of-change>10</max-rate-of-change>
    </filter>
    <filter>
        <type>noise-spike</type>
        <input>controls/engines/engine[0]/starter</input>
        <output>controls/engines/engine[0]/starter-pos</output>
        <max-rate-of-change>10</max-rate-of-change>
    </filter>
    <filter>
        <type>noise-spike</type>
        <input>controls/engines/engine[1]/starter</input>
        <output>controls/engines/engine[1]/starter-pos</output>
        <max-rate-of-change>10</max-rate-of-change>
    </filter>

    <filter>
        <type>noise-spike</type>
        <input>controls/fuel/tank[0]/boost-pump</input>
        <output>controls/fuel/tank[0]/boost-pump-pos</output>
        <max-rate-of-change>10</max-rate-of-change>
    </filter>

    <filter>
        <type>noise-spike</type>
        <input>controls/fuel/tank[1]/boost-pump</input>
        <output>controls/fuel/tank[1]/boost-pump-pos</output>
        <max-rate-of-change>10</max-rate-of-change>
    </filter>

    <filter>
        <type>gain</type>
        <gain>1</gain>
        <input>
            <expression>
                <mod>
                    <sum>
                        <mod>
                            <property>controls/fuel/tank-gauge-sel</property>
                            <value>4</value>
                        </mod>
                        <value>4</value>
                    </sum>
                    <value>4</value>
                </mod>
            </expression>
        </input>
        <output>controls/fuel/tank-gauge</output>
    </filter>
    <filter>
        <type>noise-spike</type>
        <input>controls/fuel/tank-gauge-sel</input>
        <output>controls/fuel/tank-gauge-pos</output>
        <max-rate-of-change>10</max-rate-of-change>
    </filter>

    <filter>
        <type>gain</type>
        <input>
            <condition>
                <equals>
                    <property>controls/fuel/tank-gauge</property>
                    <value>0</value>
                </equals>
            </condition>
            <property>consumables/fuel/tank[1]/level-gal_us</property>
        </input>
        <input>
            <condition>
                <equals>
                    <property>controls/fuel/tank-gauge</property>
                    <value>1</value>
                </equals>
            </condition>
            <property>consumables/fuel/tank[3]/level-gal_us</property>
        </input>
        <input>
            <condition>
                <equals>
                    <property>controls/fuel/tank-gauge</property>
                    <value>2</value>
                </equals>
            </condition>
            <property>consumables/fuel/tank[2]/level-gal_us</property>
        </input>
        <input>
            <condition>
                <equals>
                    <property>controls/fuel/tank-gauge</property>
                    <value>3</value>
                </equals>
            </condition>
            <property>consumables/fuel/tank[0]/level-gal_us</property>
        </input>
        <input>
            <value>100</value>
        </input>
        <output>consumables/fuel/level-gauge</output>
    </filter>

    <filter>
        <type>noise-spike</type>
        <input>
            <expression>
                <table>
                    <property>consumables/fuel/level-gauge</property>
                    <entry>
                        <ind>0</ind><dep>-45</dep>
                    </entry>
                    <entry>
                        <ind>40</ind><dep>-25</dep>
                    </entry>
                    <entry>
                        <ind>50</ind><dep>-22</dep>
                    </entry>
                    <entry>
                        <ind>70</ind><dep>-14</dep>
                    </entry>
                    <entry>
                        <ind>100</ind><dep>-4</dep>
                    </entry>
                    <entry>
                        <ind>110</ind><dep>0</dep>
                    </entry>
                    <entry>
                        <ind>150</ind><dep>12</dep>
                    </entry>
                    <entry>
                        <ind>160</ind><dep>17</dep>
                    </entry>
                    <entry>
                        <ind>170</ind><dep>21</dep>
                    </entry>
                    <entry>
                        <ind>180</ind><dep>26</dep>
                    </entry>
                    <entry>
                        <ind>190</ind><dep>31</dep>
                    </entry>
                    <entry>
                        <ind>200</ind><dep>38</dep>
                    </entry>
                    <entry>
                        <ind>210</ind><dep>45</dep>
                    </entry>
                </table>
            </expression>
        </input>
        <max-rate-of-change>30</max-rate-of-change>
        <output>consumables/fuel/needle-gauge</output>
    </filter>

    <filter>
        <type>noise-spike</type>
        <input>fdm/jsbsim/propulsion/engine[0]/fuel-pressure-psi</input>
        <output>engines/engine[0]/fuel-psi-norm</output>
        <max-rate-of-change>20</max-rate-of-change>
    </filter>
    <filter>
        <type>noise-spike</type>
        <input>fdm/jsbsim/propulsion/engine[1]/fuel-pressure-psi</input>
        <output>engines/engine[1]/fuel-psi-norm</output>
        <max-rate-of-change>20</max-rate-of-change>
    </filter>


    <filter>
        <type>gain</type>
        <gain>1</gain>
        <input>
            <expression>
                <mod>
                    <sum>
                        <mod>
                            <property>controls/fuel/left-valve-sel</property>
                            <value>5</value>
                        </mod>
                        <value>5</value>
                    </sum>
                    <value>5</value>
                </mod>
            </expression>
        </input>
        <output>controls/fuel/left-valve</output>
    </filter>
    <filter>
        <type>noise-spike</type>
        <input>controls/fuel/left-valve-sel</input>
        <output>controls/fuel/left-valve-pos</output>
        <max-rate-of-change>10</max-rate-of-change>
    </filter>
    <filter>
        <type>gain</type>
        <gain>1</gain>
        <input>
            <expression>
                <mod>
                    <property>controls/fuel/right-valve-sel</property>
                    <value>5</value>
                </mod>
            </expression>
        </input>
        <output>controls/fuel/right-valve</output>
    </filter>
    <filter>
        <type>noise-spike</type>
        <input>controls/fuel/right-valve-sel</input>
        <output>controls/fuel/right-valve-pos</output>
        <max-rate-of-change>10</max-rate-of-change>
    </filter>
</PropertyList>
